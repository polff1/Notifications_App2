<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.notifitions" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="res\icon\android\ic_launcher.png" />
    <name>FelipPol_NotificationsApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-local-notification" spec="^0.9.0-beta.1" />
    <plugin name="cordova-plugin-datetime-picker" spec="^0.5.1" />
    <plugin name="cordova-sqlite-storage" spec="^2.1.2" />
    <plugin name="cordova-plugin-datepicker" spec="^0.9.3" />
</widget>
